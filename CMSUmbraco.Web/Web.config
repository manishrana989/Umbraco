    <add key="Umbraco.Core.ConfigurationStatus" value="8.15.2" />
    <add key="Umbraco.Core.ReservedUrls" value="" />
    <add key="Umbraco.Core.ReservedPaths" value="/hangfire" />
    <add key="Umbraco.Core.Path" value="~/umbraco" />
