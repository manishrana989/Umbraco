      </providers>
    </membership>
    <!-- Role Provider -->
    <roleManager enabled="true" defaultProvider="UmbracoRoleProvider">
      <providers>
        <clear />
        <add name="UmbracoRoleProvider" type="Umbraco.Web.Security.Providers.MembersRoleProvider" />
      </providers>
    </roleManager>
    <machineKey validationKey="C581F74D28DB501CF00DAFA639CF08E2AE1307C40E281B188EC4819520B688F6" decryptionKey="A7BB0D27051AD1A1239712C2B397C05C240CA9C394376B6FC72364E0EE71FFD0" validation="HMACSHA256" decryption="AES" />
