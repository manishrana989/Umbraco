    <add key="Umbraco.ModelsBuilder.Enable" value="false" />
    <add key="Umbraco.ModelsBuilder.ModelsMode" value="Nothing" />
    <!-- 2FA settings -->
    <add key="TwoFactor.AppName" value="GlobalCMS" />
    <!-- uSync settings -->
    <add key="uSync:Server" value="Development" />
    <add key="uSyncPublish:AppKey" value="Not for Git" />
    <!-- IMPORTANT: MUST start with a / or it doesn't work -->
    <add key="uSync.MediaFolder" value="/media" />
    <!-- All allows all changes, otherwise set to a project code to restrict changes -->
    <add key="GlobalCMS.SchemaChangesForProjectCode" value="All" />
    <add key="Realm:ProjectId" value="5fff11f9cd77b45782b4c5c1" />
    <add key="Realm:ClusterName" value="DP-GlobalCMS" />
    <add key="Realm:ClusterLocation" value="US-VA" />
    <add key="Realm:ApiKey" value="qkkjyyka" />
    <add key="Realm:PrivateApiKey" value="251af7c0-df06-495c-8a2a-766b79f9e9b9" />
    <!-- External API / Swagger -->
    <add key="GlobalCMS.EnableExternalApi" value="true" />
    <add key="GlobalCMS.EnableSwagger" value="true" />
  </appSettings>
